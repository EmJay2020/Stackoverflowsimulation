<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <div class="container" style="margin-top:20px">
        <form method="post">
            <div class="row">
                <div class="col-md-offset-4 col-md-6">
                    <h2>Log In Page</h2>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-2">
                    <h4>Email:</h4>
                </div>
                <div class="col-md-9">
                    <input id="Email" type="text" class="form-control" name="email" placeholder="Email" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2">
                    <h4>Password:</h4>
                </div>
                <div class="col-md-9">
                    <input id="Password" type="password" class="form-control" name="password" placeholder="Password" />
                </div>
            </div>


            <br />
            <div class="row">
                <div class="col-md-offset-3 col-md-6">
                    <button id="btn" class="btn btn-primary btn-block">Log In</button>
                </div>
            </div>

        </form>
    </div>
</body>
</html>

